{
  "name": "@eluvio/elv-js-helpers",
  "version": "0.0.1",
  "description": "A collection of Javascript helper functions used by several Eluvio libraries.",
  "homepage": "https://github.com/eluv-io/elv-js-helpers",
  "main": "./index.js",
  "scripts": {
    "build:publish": "npm test && ./bin/publish.sh",
    "build:dist": "webpack && uglifyjs build/dist/elv-js-helpers.js -m -o build/dist/elv-js-helpers.min.js",
    "build": "npm run build-index-js && rimraf build && buble -i src -o build && npm run build:dist",
    "build-docs": "npm run lessc && npm run jsdoc",
    "build-index-js": "node ./bin/buildIndex.js",
    "eslint": "eslint --ext .js ./src",
    "lessc": "lessc ./docsTemplate/less/style.less > ./docsTemplate/styles/style.css",
    "jsdoc": "rimraf docs && jsdoc -c .jsdoc.json",
    "test": "mocha --recursive \"./test/**/*.test.js\"",
    "test-coverage": "nyc --reporter=html --reporter=text mocha --recursive \"./test/**/*.test.js\""
  },
  "keywords": [
    "Eluvio",
    "content",
    "fabric"
  ],
  "author": "Eluvio, Inc.",
  "license": "MIT",
  "devDependencies": {
    "@eluvio/elv-docdash": "^0.0.2",
    "bootstrap": "3.4.x",
    "buble": "^0.20.0",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "fs-extra": "^10.1.0",
    "highlight.js": "10.x.x",
    "jsdoc": "^3.6.7",
    "less": "^4.1.2",
    "marked": "^4.0.14",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "process": "^0.11.10",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "uglify-js": "^3.15.4",
    "util": "^0.12.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "crocks": "^0.12.4",
    "fraction.js": "^4.2.0",
    "kind-of": "^6.0.3",
    "luxon": "^2.3.2",
    "objectmodel": "^4.3.0",
    "ramda": "^0.28.0"
  }
}
