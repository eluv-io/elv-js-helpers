{
  "name": "@eluvio/elv-js-helpers",
  "version": "0.0.3",
  "description": "A collection of Javascript helper functions used by several Eluvio libraries.",
  "homepage": "https://github.com/eluv-io/elv-js-helpers",
  "repository": {
    "type": "git",
    "url": "https://github.com/eluv-io/elv-js-helpers"
  },
  "main": "./index.js",
  "sideEffects": false,
  "scripts": {
    "build:publish": "npm test && ./bin/publish.sh",
    "build:dist": "webpack && uglifyjs build/dist/elv-js-helpers.js -m -o build/dist/elv-js-helpers.min.js",
    "build": "npm run build-index-js && rimraf build && buble -i src -o build && npm run build:dist",
    "build-index-js": "node ./bin/buildIndex.js",
    "eslint": "eslint --ext .js ./src",
    "generate-docs": "jsdoc --readme README.md --configure .jsdoc.json",
    "release": "np --contents build",
    "test": "mocha --recursive \"./test/**/*.test.js\"",
    "test-coverage": "nyc --reporter=html --reporter=text mocha --recursive \"./test/**/*.test.js\"",
    "update-dependencies": "npx npm-check-updates -u && npm install",
    "version": "npm run generate-docs && npm run build && git add docs && git commit -m 'Update docs'"
  },
  "keywords": [
    "Eluvio",
    "content",
    "fabric"
  ],
  "author": "Eluvio, Inc.",
  "license": "MIT",
  "devDependencies": {
    "@eluvio/elv-ramdoc": "^0.0.8",
    "buble": "^0.20.0",
    "chai": "^4.3.6",
    "eslint": "^8.14.0",
    "jsdoc": "^3.6.10",
    "marked": "^4.0.14",
    "mocha": "^9.2.2",
    "np": "^7.6.1",
    "npm-check-updates": "^12.5.9",
    "nyc": "^15.1.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "uglify-js": "^3.15.4",
    "util": "^0.12.4",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "crocks": "^0.12.4",
    "fraction.js": "^4.2.0",
    "kind-of": "^6.0.3",
    "luxon": "^2.3.2",
    "objectmodel": "^4.3.0",
    "ramda": "^0.28.0"
  }
}
