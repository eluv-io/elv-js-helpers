{
  "name": "@eluvio/elv-js-helpers",
  "version": "0.0.1",
  "description": "A collection of Javascript helper functions used by several Eluvio libraries.",
  "homepage": "https://github.com/eluv-io/elv-js-helpers",
  "main": "./src/index.js",
  "scripts": {
    "build-docs": "npm run lessc && npm run jsdoc",
    "build-index-js": "node buildIndex.js",
    "lessc": "lessc --autoprefix  ./docsTemplate/less/style.less > ./docsTemplate/styles/style.css",
    "jsdoc": "jsdoc -c .jsdoc.json -r -R README.md index.js",
    "test": "mocha --recursive \"./test/**/*.test.js\"",
    "test-coverage": "nyc --reporter=html --reporter=text mocha --recursive \"./test/**/*.test.js\""
  },
  "keywords": [
    "Eluvio",
    "content",
    "fabric"
  ],
  "author": "Eluvio, Inc.",
  "license": "MIT",
  "devDependencies": {
    "@eluvio/elv-docdash": "^0.0.2",
    "bootstrap": "3.4.x",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "fs-extra": "^10.1.0",
    "handlebars": "4.x.x",
    "highlight.js": "10.x.x",
    "jsdoc": "^3.6.7",
    "less": "2.7.x",
    "less-plugin-autoprefix": "1.5.x",
    "less-plugin-clean-css": "1.5.x",
    "marked": "^4.0.14",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "pug": "^3.0.2"
  },
  "dependencies": {
    "crocks": "^0.12.4",
    "fraction.js": "^4.2.0",
    "kind-of": "^6.0.3",
    "luxon": "^2.3.2",
    "objectmodel": "^4.3.0",
    "ramda": "^0.28.0"
  }
}
